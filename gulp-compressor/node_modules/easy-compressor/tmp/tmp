            {{readable_date}}
        </div>

        <div class="full-history-list-column history-btn">
            {{#if isOwner}}
            <span class="icon-remove history-remove-btn" title="Remove Analysis"> </span>
            {{/if}}
            {{#if isCurrent}}
            <span class="icon-disk save-analysis" title="Save Analysis"> </span>
            {{/if}}

        </div>

    </div>
</script>


