    "rimraf": "^2.4.4",
    "semantic-release": "^6.0.3",
    "standard": "^7.0.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  },
  "keywords": [
    "momentjs",
    "date",
